<h2>Create a Private Game or Tournament</h2>
<form action="<%=tournament_path(@conn, :create) %>" method="POST" name="tournament" id="new-tournament">
</form>
<script>

PageData = {friends:[]};
<%= for friend <- @conn.assigns[:friends] do %>
  PageData.friends.push( {user_id: <%= friend.id %>, username: "<%= friend.username %>", selected: false });
<% end %>
</script>
