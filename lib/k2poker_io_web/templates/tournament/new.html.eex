<h2>Create a Private Game or Tournament</h2>
<form action="<%=tournament_path(@conn, :create) %>" method="POST" name="tournament" id="new-tournament">
</form>
<script>
PageData = {friends:[]};
<%= for {user_id, username} <- @conn.assigns[:friends] do %>
 PageData.friends.push( {user_id: <%= user_id %>, username: "<%= username %>", selected: false });
<% end %>
</script>
