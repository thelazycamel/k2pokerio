<%= link "Logout", to: session_path(@conn, :delete), method: :delete %>
<form
  action="<%= profile_path(@conn, :update, @profile.id) %>"
  method="post"
  name="profile"
  id="edit-profile"
  data-username="<%= @profile.username %>"
  data-blurb="<%= @profile.blurb %>"
  data-image="<%= @profile.image %>" >
</form>
